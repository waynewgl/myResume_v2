
<%= stylesheet_link_tag    "resume2", :media => "all" %>

<script type="text/javascript">
    $(document).ready(function(){

        $(".btn-slide").click(function(){
            $("#panel").slideToggle("slow");
            $(this).toggleClass("active"); return false;
        });


    });
</script>




<div id="panel">


  <div class="nav_menu"  style="    border-bottom: none;  ">
    <%= render :partial => 'partials/resume/resume_detail_menu' %>

  </div>

</div>

<p class="slide"><a href="#" class="btn-slide">MENU</a></p>


<div id="doc2" class="yui-t7" style="position: relative;top: -100px;">
  <div id="inner">

    <div id="hd">
      <div class="yui-gc">
        <div class="yui-u first">
          <h1><%= @user_avatar_ori.name %></h1>
          <h2><%=raw @user_avatar_ori.developer_field%></h2>
        </div>

        <div class="yui-u">
          <div class="contact-info">
            <h3>

              <% if !@user_avatar_ori.avatar_file_name.nil?  %>

                  <div id="gallery_wrapper" style="position: relative;top: -30px;">

                    <%= link_to image_tag(@user_avatar_ori.avatar.url(:small)),"/" + t('admin.avatar-upload-dir') + "#{@user_avatar_ori.id}/content_" + @user_avatar_ori.avatar_file_name, :title => @user_avatar_ori.avatar_file_name , :class => "myFancybox2" %>


                  </div>

              <% end %>

            </h3>
            <h3><a href="mailto:<%= raw @user_avatar_ori.email %>">

              Email:<%= raw @user_avatar_ori.email %>
            </a></h3>
            <h3> Phone#:<%= raw @user_avatar_ori.phone %></h3>
            <h3>Hobbies: <%=raw @user_avatar_ori.hobby%></h3>

          </div><!--// .contact-info -->
        </div>
      </div><!--// .yui-gc -->
    </div><!--// hd -->

    <div id="bd">
      <div id="yui-main">
        <div class="yui-b">

          <div class="yui-gf">
            <div class="yui-u first">
              <h2>Profile</h2>
            </div>
            <div class="yui-u">
              <p class="enlarge">
                <%= raw @user_avatar_ori.intro %>
            </div>
          </div><!--// .yui-gf -->

          <%if !@user_avatar_ori.other_skills.nil?  %>

              <div class="yui-gf">
                <div class="yui-u first">
                  <h2>Technical</h2>
                </div>
                <div class="yui-u">

                  <%for other_skill in @user_avatar_ori.other_skills.to_s.split("@") %>

                      <div class="talent">
                        <%= raw other_skill %>
                      </div>
                  <%end%>
                </div>
              </div><!--// .yui-gf -->

          <%end%>

          <div class="yui-gf">
            <div class="yui-u first">
              <h2>Skills</h2>
            </div>
            <div class="yui-u">

              <% if !@selected_force.nil? %>
                  <% count_Skills=1
                     for skills in @selected_force%>

                      <ul class="talent">
                       <li><%=@selected_force[count_Skills]%></li>
                        <li><%=@selected_force[count_Skills+1]%></li>
                        <li class="last"><%=@selected_force[count_Skills+2]%></li>
                      </ul>

                     <% count_Skills=count_Skills+3
                        if  count_Skills> @selected_force.length
                          break
                        end
                  end%>
              <% end %>
            </div>
          </div><!--// .yui-gf-->


          <div class="yui-gf">
            <div class="yui-u first">
              <h2>Experience</h2>
            </div>
            <div class="yui-u">
              <p class="enlarge">
                <%= raw @user_avatar_ori.experience %>
            </div>
          </div><!--// .yui-gf -->




          <div class="yui-gf">
            <div class="yui-u first">
              <h2>Education</h2>
            </div>
            <div class="yui-u">

                <p><%=raw @user_avatar_ori.qalification%>
            </div>
          </div><!--// .yui-gf -->

          <div class="yui-gf">
            <div class="yui-u first">
              <h2>Referees</h2>
            </div>
            <div class="yui-u">

              <%=raw @user_avatar_ori.referees%>
            </div>
          </div><!--// .yui-gf -->


          <div class="yui-gf">
            <div class="yui-u first">
              <h2>Projects</h2>
            </div>
            <div class="yui-u">

                <%= link_to "Click to see all projects I have done" , {:controller=>:site, :action => :allProjects , :user_project_id=> @user_avatar_ori.id }, :style =>"color: #000000;" %>


            </div>
          </div><!--// .yui-gf -->





        </div><!--// .yui-b -->



      </div><!--// yui-main -->
    </div><!--// bd -->

    <div id="ft">
    </div><!--// footer -->

  </div><!-- // inner -->


</div><!--// doc -->


